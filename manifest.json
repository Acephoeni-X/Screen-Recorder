{

    "manifest_version": 2,
    "name": "Screen Capture",
    "version": "1.0",
  
    "description": "Screen Recorder",
  
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["Content.js"]
      }
    ]
    ,

    "browser_action": {
      "default_popup": "index.html",
      "default_title": "Screen Recorder" 
    }
  
  }